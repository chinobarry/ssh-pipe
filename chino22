pipeline {
    agent any

    stages {
        stage("build") {
            steps {
                echo 'building my application...'

            }

        }    


        stage("test") { 
            steps {
                git url: 'https://github.com/chinobarry/chino-pipeline.git'
            }
        }

        stage('deploy') {
            steps {
                echo 'deploying my application...'

            }    

        } 
    }
}                          
